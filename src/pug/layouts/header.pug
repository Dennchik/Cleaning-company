								span Услуги
								ul.header__sub-item 
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span управление арендными отношениями
									li.header__sub-link
										a(href="#management").anchor-link.link_underline-lr 
											span эксплуатация и обслуживание
									li.header__sub-link
										a(href="#brokerage").anchor-link.link_underline-lr 
											span брокеридж
									li.header__sub-link
										a(href="#cleaning").anchor-link.link_underline-lr 
											span пожарная безопасность
									li.header__sub-link
										a(href="#cleaning").anchor-link.link_underline-lr 
											span клининг
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span технический аудит
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span аварийно-диспетчерская служба 
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span доверительное управление
						li.header__item
							.header__link-key.link_underline-lr
								span Как работаем
								ul.header__sub-item 
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span финансовое планирование 
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span бухгалтерский и кадровый учет
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span юридическое сопровождение 
						li.header__item
							.header__link-key.link_underline-lr
								span Примеры работ
								ul.header__sub-item 
									li.header__sub-link
										a(href="#prices").anchor-link.link_underline-lr 
											span собственникам 
									li.header__sub-link
										a(href="#client").anchor-link.link_underline-lr 
											span инвесторам
									li.header__sub-link
										a(href="#client").anchor-link.link_underline-lr 
											span арендаторам
									li.header__sub-link
										a(href="#client").anchor-link.link_underline-lr 
											span управляющим компаниями 
						li.header__item
							a(href="#contacts").header__link-key.anchor-link.link_underline-lr 
								span Оставить заявку 
			.header__column
				.header__phone 
					.header__phone-item._log-in  
